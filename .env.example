# Plaid Banking API (get from https://dashboard.plaid.com/)
PLAID_CLIENT_ID=your_plaid_client_id_here
PLAID_SECRET=your_plaid_secret_here
PLAID_ENV=sandbox
PLAID_WEBHOOK_SECRET=your_webhook_secret_here

# OpenAI API (get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=your_openai_api_key_here

# Redis Database
REDIS_URL=redis://localhost:6379

# Upstash Redis for Edge Runtime (get from https://console.upstash.com/)
UPSTASH_REDIS_REST_URL=your_upstash_rest_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_rest_token_here

# Authentication
SESSION_SECRET=your_super_secret_session_key_here_use_at_least_32_characters

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Optional Cache Settings
CACHE_TTL_DASHBOARD=300
CACHE_TTL_SEARCH=60